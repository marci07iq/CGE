<body>
	<div
		id = "objectPluginCreate"
		bgcolor = "#BF2D2D30"
	>
		<location>
			<bot
				rel="0.0"
				abs="0.0"
			></bot>
			<top
				rel="1.0"
				abs="0.0"
			></top>
			<left
				rel="0.0"
				abs="0.0"
			></left>
			<right
				rel="0.0"
				abs="200.0"
			></right>
		</location>
		<label
			id = "objectPluginCreatePositionLabel"
			align = "0"
		>
			<location>
				<bot
					rel="1.0"
					abs="-60.0"
				></bot>
				<top
					rel="1.0"
					abs="-30.0"
				></top>
				<left
					rel="0.0"
					abs="10.0"
				></left>
				<right
					rel="1.0"
					abs="0.0"
				></right>
		</location>Position</label>
		<input
			id = "objectPluginCreatePositionXInput"
			inputfunc = "pluginCreatePositionXInput"
			validatorfunc = "floatValidator"
		>
			<location>
				<bot
					rel="1.0"
					abs="-80.0"
				></bot>
				<top
					rel="1.0"
					abs="-60.0"
				></top>
				<left
					rel="0.0"
					abs="10.0"
				></left>
				<right
					rel="1.0"
					abs="0.0"
				></right>
			</location>0</input>
		<input
			id = "objectPluginCreatePositionYInput"
			inputfunc = "pluginCreatePositionYInput"
			validatorfunc = "floatValidator"
		>
			<location>
				<bot
					rel="1.0"
					abs="-100.0"
				></bot>
				<top
					rel="1.0"
					abs="-80.0"
				></top>
				<left
					rel="0.0"
					abs="10.0"
				></left>
				<right
					rel="1.0"
					abs="0.0"
				></right>
			</location>0</input>
		<input
			id = "objectPluginCreatePositionZInput"
			inputfunc = "pluginCreatePositionZInput"
			validatorfunc = "floatValidator"
		>
			<location>
				<bot
					rel="1.0"
					abs="-120.0"
				></bot>
				<top
					rel="1.0"
					abs="-100.0"
				></top>
				<left
					rel="0.0"
					abs="10.0"
				></left>
				<right
					rel="1.0"
					abs="0.0"
				></right>
			</location>0</input>
			
		<label
			id = "objectPluginCreateSizeLabel"
			align = "0"
		>
			<location>
				<bot
					rel="1.0"
					abs="-180.0"
				></bot>
				<top
					rel="1.0"
					abs="-150.0"
				></top>
				<left
					rel="0.0"
					abs="10.0"
				></left>
				<right
					rel="1.0"
					abs="0.0"
				></right>
		</location>Size</label>
		<input
			id = "objectPluginCreateSizeXInput"
			inputfunc = "pluginCreateSizeXInput"
			validatorfunc = "floatValidator"
		>
			<location>
				<bot
					rel="1.0"
					abs="-200.0"
				></bot>
				<top
					rel="1.0"
					abs="-180.0"
				></top>
				<left
					rel="0.0"
					abs="10.0"
				></left>
				<right
					rel="1.0"
					abs="0.0"
				></right>
			</location>1</input>
		<input
			id = "objectPluginCreateSizeYInput"
			inputfunc = "pluginCreateSizeYInput"
			validatorfunc = "floatValidator"
		>
			<location>
				<bot
					rel="1.0"
					abs="-220.0"
				></bot>
				<top
					rel="1.0"
					abs="-200.0"
				></top>
				<left
					rel="0.0"
					abs="10.0"
				></left>
				<right
					rel="1.0"
					abs="0.0"
				></right>
			</location>1</input>
		<input
			id = "objectPluginCreateSizeZInput"
			inputfunc = "pluginCreateSizeZInput"
			validatorfunc = "floatValidator"
		>
			<location>
				<bot
					rel="1.0"
					abs="-240.0"
				></bot>
				<top
					rel="1.0"
					abs="-220.0"
				></top>
				<left
					rel="0.0"
					abs="10.0"
				></left>
				<right
					rel="1.0"
					abs="0.0"
				></right>
			</location>1</input>
			
			
		<button
			id = "objectPluginCreateDoneButton"
			callback = "pluginCreateDoneButton"
		>
			<location>
				<bot
					rel="1.0"
					abs="-300.0"
				></bot>
				<top
					rel="1.0"
					abs="-270.0"
				></top>
				<left
					rel="0.0"
					abs="10.0"
				></left>
				<right
					rel="1.0"
					abs="0.0"
				></right>
			</location>Done</button>
	</div>
</body>